<div id="headerBox">
  <div class="logoMain">
    <%= link_to logo, root_path %>
  </div>
  <div class="headerSpace">
    <div class="fb-like" data-href="https://www.facebook.com/webprimmer" data-width="The pixel width of the plugin" data-height="The pixel height of the plugin" data-colorscheme="dark" data-layout="button_count" data-action="like" data-show-faces="false" data-send="false"></div>
  </div>
  <div class="headerSpaceTwo">
    <div class="userNameText"></div>
    <div class="userNameText"></div>
  </div>
  <div class="loginBoxes">
    <form>
      <input class="text" type="text" maxlength="40" size="30" tabindex="1" id="userName" placeholder="username or email"/><br>
      <input class="text" type="password" maxlength="20" size="30" tabindex="2" id="pass" placeholder="password"/>
    </form>
  </div>
</div>